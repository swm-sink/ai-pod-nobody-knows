{
  "mcpServers": {
    "elevenlabs": {
      "command": "python3",
      "args": ["-m", "elevenlabs_mcp.server"],
      "env": {
        "ELEVENLABS_API_KEY": "${ELEVENLABS_API_KEY}"
      }
    },
    "perplexity": {
      "command": "node",
      "args": [".claude/mcp-servers/perplexity-mcp/perplexity-ask/dist/index.js"],
      "env": {
        "PERPLEXITY_API_KEY": "${PERPLEXITY_API_KEY}"
      }
    }
  }
}
